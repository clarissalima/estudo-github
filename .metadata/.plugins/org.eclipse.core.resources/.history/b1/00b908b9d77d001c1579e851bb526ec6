package entities;

public class Product {
	private String name;
	private String email;
	private int n;
	
	public Product(String name, String email, int n) {
		this.name = name;
		this.email = email;
		this.n = n;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getEmail() {
		return email;
	}

	public void setEmail(String email) {
		this.email = email;
	}

	public int getN() {
		return n;
	}

	public void setN(int n) {
		this.n = n;
	}
	
	
	
}
